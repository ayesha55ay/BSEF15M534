print("THis is my first  FCFS program in Python")

proc={}		//here we have dictionary to contain incoming proc
a_time=[]	//list for incoming arrival time
b_time=[]	//list for inccoming burst time

for i in range (0,4):
num=input("Enter arrival time of processes")
if(i==0):
	initial=num

elif(initial>num)
	initial=num

a_time.append(num)

num2=input("Enter burst time for cpu processes")
b_time.append(num2)

proc[i+1]=[a_time[i],b_time[i]]
print "Arrival.T","		","Burst.T"

for j in range (1,5):
print proc.get(j)[0],"		",print proc.get(j)[1]
totaltime=initial
if(totaltime>0):
print "0---------",totaltime, "Idle time"
for j in range(1,5):
b_time=proc.get(j)[1]
totaltime=totaltime+b_time
print initial,"		 ", totaltime ," 		 ", j ," completed "
initial=totaltime








